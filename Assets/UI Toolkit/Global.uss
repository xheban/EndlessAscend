    padding-right: 24px;
    padding-bottom: 16px;
    padding-left: 24px;
    flex-direction: column-reverse;
}

.footer.footer-hovering {
    -unity-background-image-tint-color: var(--color-hover-success);
}

.footer-active {
    -unity-background-image-tint-color: var(--color-item-selected);
}

IntegerField > #unity-text-input {
    padding-left: 4px;

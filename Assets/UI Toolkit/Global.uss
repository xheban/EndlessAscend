}

.tint {
    cursor: pointer;
}

.tint:disabled {
    opacity: 0.28;
    cursor: default;
}

.tint:enabled:hover {
    background-color: rgba(255, 255, 255, 0);
    -unity-background-image-tint-color: var(--color-hover-success);
}

.combat-log-line {
